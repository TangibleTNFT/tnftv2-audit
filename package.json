{
  "name": "tangible-nft",
  "version": "0.1.0",
  "description": "TangibleNFT Marketplace",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "start": "npx hardhat deploy",
    "start:mumbai": "npx hardhat deploy --network mumbai --gasprice 20000000000",
    "start:rinkeby": "npx hardhat run scripts/deployAll.js --network rinkeby",
    "prettier": "prettier --write --plugin=prettier-plugin-solidity '**/*.sol'",
    "prettierOne": "prettier --write --plugin=prettier-plugin-solidity ",
    "lint": "prettier --list-different 'contracts/**/*.sol'"
  },
